{
  "kind": "collectionType",
  "collectionName": "all_sites",
  "info": {
    "singularName": "all-site",
    "pluralName": "all-sites",
    "displayName": "_allSite",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "siteName": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "siteDomain": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "targetLink": {
      "type": "string",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "siteID": {
      "type": "uid",
      "required": true
    },
    "editor_info": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::editor-info.editor-info",
      "inversedBy": "all_sites"
    },
    "faq": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::faq.faq",
      "inversedBy": "all_sites"
    },
    "home_page": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::home-page.home-page",
      "inversedBy": "all_sites"
    },
    "siteLogo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "siteTitle": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "siteDescription": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "favicon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "page_sections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::page-section.page-section",
      "mappedBy": "all_sites"
    },
    "themePrimaryColor": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "themeSecondaryColor": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "targetLinkButton": {
      "type": "string",
      "default": "/",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "footer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::footer.footer",
      "inversedBy": "all_sites"
    },
    "header": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::header.header",
      "inversedBy": "all_sites"
    },
    "localeLang": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "en-Gb",
        "pl-PL",
        "es-ES",
        "nl-NL",
        "fr-FR",
        "de-DE"
      ]
    },
    "themeBGPrimaryColor": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "default": "#10181f"
